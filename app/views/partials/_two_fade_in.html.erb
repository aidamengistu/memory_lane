<div id='content_partial_<%= p[:id] %>' class="row twofadein">
  <div class="delete-partial-x-styling delete-partial-x" data-idx="<%= p[:id] %>"><span class="glyphicon glyphicon-remove"></span></div>
  <div class="col-left-half" data-sr='enter left, over 3s, reset' >
    <a href="#" class="upload_widget_opener" data-idx="<%= p.id %>" data-item="1">
      <%= image_tag "#{p[:item1]}", class: "img-responsive image-fit" %>
    </a>
  </div>
  <div class="col-right-half" data-sr='enter right, over 3s, reset' >
    <a href="#" class="upload_widget_opener" data-idx="<%= p.id %>" data-item="2">
      <%= image_tag "#{p[:item2]}", class: "img-responsive image-fit" %>
    </a>
  </div>
</div>
